<script type="application/javascript">
	function refresh_counts() {
		$(".zone-count").each(function(id, zone) {
			$.ajax(
				{
					url: "/info_parking?zone=" + $(zone).attr("data"),
					complete: function(data) {
						$(zone).text(data.responseText);
					}
				}
			);
		});
	}
	
	function search(){
		var locationZ = '/zone?zone=' + $('#search').val();
		window.location.href= locationZ;
	}

	$(document).ready(function() {
		refresh_counts();
		setInterval(refresh_counts, 2000);
	});
</script>
